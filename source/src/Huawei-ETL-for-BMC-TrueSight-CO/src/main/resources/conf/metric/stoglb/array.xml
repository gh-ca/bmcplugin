    </metric>
    <metric dataSet="STOGLB" column="ST_OVERSUBSCRIBED_CAPACITY">
        <tasks>
            <task id="0" method="com.huawei.storage.utils.TaskHandler.getFromRestContext">
                <target>THICKLUNSALLOCATECAPACITY,THINLUNSMAXCAPACITY,STORAGEPOOLCAPACITY,SECTORSIZE</target>
                <result>THICKLUNSALLOCATECAPACITY,THINLUNSMAXCAPACITY,STORAGEPOOLCAPACITY,SECTORSIZE</result>
            </task>
            <task id="1" method="com.huawei.storage.utils.TaskHandler.addValues">
                <target>THICKLUNSALLOCATECAPACITY,THINLUNSMAXCAPACITY</target>
                <result>SUBSCRIBED_CAPACITY</result>
            </task>
            <task id="2" method="com.huawei.storage.utils.TaskHandler.getDifference">
                <target>SUBSCRIBED_CAPACITY,STORAGEPOOLCAPACITY</target>
                <result>OVER_CAPACITY</result>
            </task>
            <task id="3" method="com.huawei.storage.utils.TaskHandler.evalExpression">
                <target>${OVER_CAPACITY},*,${SECTORSIZE}</target>
                <result>ST_OVERSUBSCRIBED_CAPACITY</result>
            </task>
        </tasks>
    </metric>
    <metric dataSet="STOGLB" column="ST_AVAILABLE_CAPACITY">
